<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="project_task_view_form" model="ir.ui.view">
        <field name="name">project.task.view.form.inherit.project.enterprise</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_form2"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='planned_date_begin']" position="replace">
                <field name="planned_date_begin"/>
            </xpath>
            <xpath expr="//field[@name='planned_date_end']" position="replace">
                <field name="planned_date_end"/>
            </xpath>
        </field>
    </record>
</odoo>